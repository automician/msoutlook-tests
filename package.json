{
  "name": "msoutlook-test",
  "version": "1.0.0",

  "config": {
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },

  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "convert-to-single-host": "node convertToSingleHost.js",
    "dev-server": "webpack serve --mode development --no-client-overlay",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:desktop:outlook": "office-addin-debugging start ./src/manifest-localhost.xml desktop --app outlook",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "test:e2e:metro:android": "cross-env TEST_PLATFORM=android jest --config __e2e__/jest.config.js",
    "test:e2e:windows": "cross-env TEST_PLATFORM=windows jest --config __e2e__/jest.config.js",
    "test:e2e:macos": "cross-env TEST_PLATFORM=macos jest --config __e2e__/jest.config.js",
    "test:e2e:metro:ios": "cross-env TEST_PLATFORM=ios jest --config __e2e__/jest.config.js"
  },
  "dependencies": {
    "@expo/webpack-config": "^18.0.1",
    "@fluentui/react-components": "^9.47.5",
    "@microsoft/office-js": "^1.1.91",
    "@wdio/globals": "^8.33.1",
    "appium": "^2.5.1",
    "axios": "^1.4.0",
    "dotenv": "^16.0.3",
    "expo": "~48.0.15",
    "expo-status-bar": "~1.4.4",
    "node": "^21.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.7",
    "react-native-web": "~0.18.10",
    "@fluentui/react-icons": "^2.0.214",
    "core-js": "^3.36.0",
    "es6-promise": "^4.2.8",
    "regenerator-runtime": "^0.14.1"

  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@faker-js/faker": "^8.4.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.4",
    "@types/react": "~18.0.14",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
